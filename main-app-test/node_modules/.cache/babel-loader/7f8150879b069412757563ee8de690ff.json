{"ast":null,"code":"// import mainmenu from \"@/components/mainmenu.vue\"\n// import firstpage from \"@/pages/firstpage.vue\";\nimport navmenu from \"@/pages/navmenu.vue\"; // import newmenu from \"@/pages/newmenu.vue\"\n\nexport default {\n  data() {\n    return {\n      onpage: false,\n      userlogin: \"\"\n    };\n  },\n\n  components: {\n    // firstpage,\n    navmenu\n  },\n\n  beforeCreate() {//localStorage.setItem(\"onpage\", 0)\n  },\n\n  beforeUnmount() {\n    sessionStorage.clear();\n  },\n\n  mounted() {\n    this.$refs.user_input.focus();\n  },\n\n  methods: {\n    toLogin() {\n      sessionStorage.setItem(\"user_login\", \"admin\");\n      this.userlogin = sessionStorage.getItem(\"user_login\");\n      this.onpage = !this.onpage;\n    },\n\n    toLogout() {\n      this.$router.replace(\"/\"); // ถ้าต้องการ clear session เฉพาะบางตัว ให้ระบุตามบรรทัดด้านล่าง\n      // sessionStorage.removeItem(\"user_login\")\n      // clear session ที่สร้างขึ้นมาทั้งหมด\n\n      sessionStorage.clear();\n      this.onpage = !this.onpage;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsDA;AACA;AACA,OAAOA,OAAP,MAAoB,qBAApB,C,CACA;;AAEA,eAAe;EACXC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE,KADL;MAEHC,SAAS,EAAE;IAFR,CAAP;EAIH,CANU;;EAOXC,UAAU,EAAE;IACR;IACAJ;EAFQ,CAPD;;EAWXK,YAAY,GAAG,CACX;EACH,CAbU;;EAcXC,aAAa,GAAG;IACZC,cAAc,CAACC,KAAf;EACH,CAhBU;;EAiBXC,OAAO,GAAG;IACN,KAAKC,KAAL,CAAWC,UAAX,CAAsBC,KAAtB;EACH,CAnBU;;EAoBXC,OAAO,EAAE;IACLC,OAAO,GAAG;MACNP,cAAc,CAACQ,OAAf,CAAuB,YAAvB,EAAoC,OAApC;MACA,KAAKZ,SAAL,GAAiBI,cAAc,CAACS,OAAf,CAAuB,YAAvB,CAAjB;MACA,KAAKd,MAAL,GAAc,CAAC,KAAKA,MAApB;IACH,CALI;;IAMLe,QAAQ,GAAG;MACP,KAAKC,OAAL,CAAaC,OAAb,CAAqB,GAArB,EADO,CAEP;MACA;MACA;;MACAZ,cAAc,CAACC,KAAf;MACA,KAAKN,MAAL,GAAc,CAAC,KAAKA,MAApB;IACH;;EAbI;AApBE,CAAf","names":["navmenu","data","onpage","userlogin","components","beforeCreate","beforeUnmount","sessionStorage","clear","mounted","$refs","user_input","focus","methods","toLogin","setItem","getItem","toLogout","$router","replace"],"sourceRoot":"","sources":["D:\\Project-vue\\SPR-PROJECT\\main-app-test\\src\\App.vue"],"sourcesContent":["<template>\r\n    <!-- <div class=\"container-fluid mt-2\"> -->\r\n    <nav class=\"navbar bg-primary bg-opacity-25 shadow rounded-3\">\r\n        <a class=\"navbar-brand ms-2\">\r\n            <img src=\"./assets/sprlogo1x1.png\" alt=\"Logo\" width=\"25\" height=\"25\"\r\n                class=\"d-inline-block align-text-top\" />\r\n            <span class=\"navbar-brand mb-0 ms-2\" style=\"color: blue\" v-if=\"this.onpage == true\">\r\n                <b style=\"color: blue\">{{ this.userlogin }}</b>\r\n                <b style=\"color: black\">[</b>\r\n                <b style=\"color: forestgreen\">online</b>\r\n                <b style=\"color: black\">]</b>\r\n            </span>\r\n        </a>\r\n        <div class=\"d-flex me-2\">\r\n            <button title=\"ออกจากระบบ\" class=\"btn btn-logout shadow\" @click.prevent=\"toLogout()\"\r\n                v-if=\"this.onpage == true\">\r\n                <font-awesome-icon icon=\"fa-solid fa-sign-out\" style=\"color: white\" />\r\n            </button>\r\n        </div>\r\n    </nav>\r\n    <!-- </div> -->\r\n    <div class=\"container-fluid\" v-if=\"this.onpage == false\">\r\n        <div class=\"card rounded-5 shadow-lg mx-auto mt-5 bg-primary bg-opacity-25\" style=\"width: 18rem\">\r\n            <img src=\"./assets/sprlogo3x2.png\" class=\"card-img-top\" alt=\"spr-logo\" />\r\n            <div class=\"card-body text-center\">\r\n                <h5 class=\"card-title\" style=\"font-size: small; font-weight: bold; color: darkblue\">\r\n                    SOMPONG RUBBER RETREADING\r\n                </h5>\r\n                <div class=\"mb-2\">\r\n                    <input type=\"text\" class=\"form-control shadow-sm\" id=\"exampleFormControlInput1\"\r\n                        placeholder=\"รหัสผู้ใช้งาน\" ref=\"user_input\" />\r\n                </div>\r\n                <div class=\"mb-2\">\r\n                    <input type=\"password\" class=\"form-control shadow-sm\" id=\"exampleFormControlInput1\"\r\n                        placeholder=\"รหัสผ่าน\" />\r\n                </div>\r\n                <div class=\"align-content-center mt-3\">\r\n                    <button class=\"btn btn-login rounded-3 shadow glow-on-hover\" @click.prevent=\"toLogin()\">\r\n                        <font-awesome-icon icon=\"fa-solid fa-user-shield\" />&nbsp;เข้าสู่ระบบ\r\n                    </button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <!-- zone view other page -->\r\n    <div class=\"container-fluid\">\r\n        <firstpage v-if=\"this.onpage == true\" />\r\n        <!-- <newmenu v-if=\"this.onpage == true\" /> -->\r\n        <!-- <mainmenu v-if=\"this.onpage == 'x'\" /> -->\r\n    </div>\r\n    <!-- end zone view other page -->\r\n</template>\r\n\r\n<script>\r\n// import mainmenu from \"@/components/mainmenu.vue\"\r\n// import firstpage from \"@/pages/firstpage.vue\";\r\nimport navmenu from \"@/pages/navmenu.vue\"\r\n// import newmenu from \"@/pages/newmenu.vue\"\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            onpage: false,\r\n            userlogin: \"\",\r\n        };\r\n    },\r\n    components: {\r\n        // firstpage,\r\n        navmenu,\r\n    },\r\n    beforeCreate() {\r\n        //localStorage.setItem(\"onpage\", 0)\r\n    },\r\n    beforeUnmount() {\r\n        sessionStorage.clear();\r\n    },\r\n    mounted() {\r\n        this.$refs.user_input.focus();\r\n    },\r\n    methods: {\r\n        toLogin() {\r\n            sessionStorage.setItem(\"user_login\",\"admin\")\r\n            this.userlogin = sessionStorage.getItem(\"user_login\")\r\n            this.onpage = !this.onpage;\r\n        },\r\n        toLogout() {\r\n            this.$router.replace(\"/\");\r\n            // ถ้าต้องการ clear session เฉพาะบางตัว ให้ระบุตามบรรทัดด้านล่าง\r\n            // sessionStorage.removeItem(\"user_login\")\r\n            // clear session ที่สร้างขึ้นมาทั้งหมด\r\n            sessionStorage.clear();\r\n            this.onpage = !this.onpage;\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style>\r\n.dataTables_filter {\r\n    width: 35%;\r\n    float: right;\r\n    text-align: left;\r\n}\r\n\r\ntr:hover {\r\n    background-color: rgb(124, 152, 253);\r\n}\r\n\r\n.btn-logout {\r\n    height: 40px;\r\n    width: 40px;\r\n    background: rgb(255, 88, 88);\r\n    /* margin: 20px; */\r\n    float: left;\r\n    box-shadow: 0 0 1px #ccc;\r\n    -webkit-transition: all 0.2s ease-in-out;\r\n    border: 0px;\r\n    color: rgb(255, 255, 255);\r\n}\r\n\r\n.btn-logout:hover {\r\n    -webkit-transform: scale(1.1);\r\n    background: #ff0000;\r\n}\r\n\r\n/* .btn-login {\r\n    background: #0088ff;\r\n    color: white;\r\n} */\r\n.btn-login:hover {\r\n    /* background: #1500ff; */\r\n    color: rgb(255, 255, 255);\r\n}\r\n\r\nhtml,\r\nbody {\r\n    background: rgb(255, 255, 255);\r\n}\r\n\r\n.glow-on-hover {\r\n    /* width: 220px;\r\n    height: 50px; */\r\n    border: none;\r\n    outline: none;\r\n    color: #fff;\r\n    background: darkblue;\r\n    cursor: pointer;\r\n    position: relative;\r\n    z-index: 0;\r\n    border-radius: 10px;\r\n}\r\n\r\n.glow-on-hover:before {\r\n    content: \"\";\r\n    background: linear-gradient(45deg,\r\n            #ff0000,\r\n            #ff7300,\r\n            #fffb00,\r\n            #48ff00,\r\n            #00ffd5,\r\n            #002bff,\r\n            #7a00ff,\r\n            #ff00c8,\r\n            #ff0000);\r\n    position: absolute;\r\n    top: -2px;\r\n    left: -2px;\r\n    background-size: 400%;\r\n    z-index: -1;\r\n    filter: blur(5px);\r\n    width: calc(100% + 4px);\r\n    height: calc(100% + 4px);\r\n    animation: glowing 20s linear infinite;\r\n    opacity: 0;\r\n    transition: opacity 0.3s ease-in-out;\r\n    border-radius: 10px;\r\n}\r\n\r\n.glow-on-hover:active {\r\n    color: darkblue;\r\n}\r\n\r\n.glow-on-hover:active:after {\r\n    background: transparent;\r\n}\r\n\r\n.glow-on-hover:hover:before {\r\n    opacity: 1;\r\n}\r\n\r\n.glow-on-hover:after {\r\n    z-index: -1;\r\n    content: \"\";\r\n    position: absolute;\r\n    width: 100%;\r\n    height: 100%;\r\n    background: darkblue;\r\n    left: 0;\r\n    top: 0;\r\n    border-radius: 10px;\r\n}\r\n\r\n@keyframes glowing {\r\n    0% {\r\n        background-position: 0 0;\r\n    }\r\n\r\n    50% {\r\n        background-position: 400% 0;\r\n    }\r\n\r\n    100% {\r\n        background-position: 0 0;\r\n    }\r\n}\r\n\r\n.third {\r\n    border-color: #3498db;\r\n    color: #fff;\r\n    box-shadow: 0 0 40px 40px #090085 inset, 0 0 0 0 #00087e;\r\n    -webkit-transition: all 150ms ease-in-out;\r\n    transition: all 150ms ease-in-out;\r\n}\r\n\r\n.third:hover {\r\n    box-shadow: 0 0 10px 0 #3498db inset, 0 0 10px 4px #3498db;\r\n}\r\n\r\n.btn-menu {\r\n    box-sizing: border-box;\r\n    -webkit-appearance: none;\r\n    -moz-appearance: none;\r\n    appearance: none;\r\n    background-color: transparent;\r\n    border-radius: 0.6em;\r\n    cursor: pointer;\r\n    display: -webkit-box;\r\n    display: -webkit-flex;\r\n    display: -ms-flexbox;\r\n    display: flex;\r\n    -webkit-align-self: center;\r\n    -ms-flex-item-align: center;\r\n    align-self: center;\r\n    font-size: 1rem;\r\n    line-height: 1;\r\n    text-decoration: none;\r\n    text-align: center;\r\n    text-transform: uppercase;\r\n    font-family: \"Montserrat\", sans-serif;\r\n}\r\n\r\n.btn-menu:hover,\r\n.btn:focus {\r\n    color: #fff;\r\n    outline: 0;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}