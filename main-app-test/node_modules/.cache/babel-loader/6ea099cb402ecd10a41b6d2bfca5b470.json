=\"modal-footer\">\r\n                    <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">ปิด</button>\r\n                    <button type=\"button\" class=\"btn btn-primary\">บันทึก</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"card border-dark border-2 shadow\" style=\"width:100% ;\">\r\n        <table id=\"showrubberstyle\" class=\"table table-striped table-hover table-sm\"\r\n            style=\"width: 100%; align-items: center\">\r\n            <thead>\r\n                <tr>\r\n                    <th>ลำดับ</th>\r\n                    <th>รหัสลายดอก</th>\r\n                    <th>รายละเอียด</th>\r\n                    <th style=\"text-align:right\">\r\n                        <button class=\"btn btn-success btn-sm me-md-2\" data-bs-toggle=\"modal\"\r\n                            data-bs-target=\"#newStyle\">\r\n                            <!-- ปุ่มเพิ่มข้อมูล -->\r\n                            <font-awesome-icon icon=\"fa-solid fa-plus\" />\r\n                        </button>\r\n                    </th>\r\n                    <th></th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"rec in data_row\" :key=\"rec.id\" @click.prevent=\"getStyle(rec)\">\r\n                    <td>{{ rec.id }}</td>\r\n                    <td>{{ rec.rub_style_id }}</td>\r\n                    <td>{{ rec.rub_style_detail }}</td>\r\n                    <td style=\"text-align:right\">\r\n                        <button id=\"edit\" type='button' class='btn btn-primary btn-sm me-md-2' @click.prevent=\"getStyle(rec)\">\r\n                            <!-- ปุ่มแก้ไขข้อมูล -->\r\n                            <font-awesome-icon icon=\"fa-solid fa-pen\" />\r\n                        </button>\r\n                    </td>\r\n                    <td style=\"text-align:left\">\r\n                        <button class='btn btn-danger btn-sm me-md-1' @click=\"hideModal('editStyleM')\">\r\n                            <!-- ปุ่มลบข้อมูล -->\r\n                            <font-awesome-icon icon=\"fa-solid fa-trash-can\" />\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n// import axios from 'axios';\r\nimport $ from 'jquery';\r\nimport axios from \"@/axios\";\r\nimport { Modal } from \"bootstrap\"\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            data_row: [],\r\n            rubberStyle: {\r\n                id: \"\",\r\n                rub_style_id: \"\",\r\n                rub_style_detail: \"\"\r\n            },\r\n            styleEdit: {\r\n                id: \"\",\r\n                rub_style_id: \"\",\r\n                rub_style_detail: \"\"\r\n            },\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getRubberStyle();\r\n    },\r\n    methods: {\r\n        async getStyle(val) {\r\n            this.styleEdit.rub_style_id = val.rub_style_id;\r\n            this.styleEdit.rub_style_detail = val.rub_style_detail;\r\n            this.showModal(\"editStyleM\");\r\n        },\r\n        async getRubberStyle() {\r\n            await axios.get(`rubberstyle/read`).then((response) => {\r\n                this.data_row = response.data;\r\n                $(document).ready(function () {\r\n                    $('#showrubberstyle').DataTable({\r\n                        \"language\": {\r\n                            \"decimal\": \"\",\r\n                            \"emptyTable\": \"ไม่พบการข้อมูลจากฐานข้อมูล\",\r\n                            \"info\": \"จำนวน _END_ เรคอร์ด / _TOTAL_ เรคอร์ด\",\r\n                            // \"info\": \"แสดง _START_ - _END_ แถว จากทั้งหมด _TOTAL_ แถว\",\r\n                            \"infoEmpty\": \"Showing 0 to 0 of 0 entries\",\r\n                            \"infoFiltered\": \"(ค้นหาจากข้อมูลทั้งหมด _MAX_ เรคอร์ด)\",\r\n                            \"infoPostFix\": \"\",\r\n                            \"thousands\": \",\",\r\n                            \"lengthMenu\": \"แสดงครั้งละ _MENU_ เรคอร์ด\",\r\n                            \"loadingRecords\": \"Loading...\",\r\n                            \"processing\": \"\",\r\n                            \"search\": \"ค้นหาข้อมูล:\",\r\n                            \"zeroRecords\": \"ไม่พบข้อมูลที่ค้นหา\",\r\n                            \"paginate\": {\r\n                                \"first\": \"First\",\r\n                                \"last\": \"Last\",\r\n                                \"next\": \"ต่อไป\",\r\n                                \"previous\": \"ย้อนกลับ\"\r\n                            },\r\n                            \"aria\": {\r\n                                \"sortAscending\": \": activate to sort column ascending\",\r\n                                \"sortDescending\": \": activate to sort column descending\"\r\n                            }\r\n                        }\r\n                    });\r\n                });\r\n            });\r\n        },\r\n        async showModal(e) {\r\n            let myModal = await new Modal(document.getElementById(e))\r\n            await myModal.show()\r\n        },\r\n        async hideModal(e) {\r\n            await $(`#${e}`).removeClass(\"in\");\r\n            await $(\".modal-backdrop\").remove();\r\n            await $(`#${e}`).hide();\r\n        },\r\n    },\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}