his.productiontype_data = response.data;\n      });\n    },\n\n    async getRubberStyle() {\n      await axios.get(`rubberstyle/read`).then(response => {\n        this.rubstyle_data = response.data;\n      });\n    },\n\n    async getRubberSize() {\n      await axios.get(`rubbersize/read`).then(response => {\n        this.rubsize_data = response.data;\n      });\n    },\n\n    async saveNewProduct() {\n      await axios.post(`productlist/checkdup`, this.new_product_list).then(response => {\n        this.check_dup = response.data;\n\n        if (this.check_dup.length == 0) {\n          if (confirm(\"ยืนยันการบันทุกข้อมูล\") == true) {\n            axios.post(`productlist/new`, this.new_product_list).then(() => {}); // $(\"#listproduct\").DataTable().destroy();\n\n            this.getProduct();\n            this.setDatable();\n          }\n        } else {\n          alert(\"พบว่ามีรายการสินค้านี้อยู่แล้ว !!!\");\n        }\n      }).catch(error => {\n        console.log(\"พบข้อผิดผลาดในการบันทึกข้อมูล \" + error);\n      });\n    },\n\n    async getProduct() {\n      await axios.get(`productlist/read`).then(response => {\n        this.product_data = response.data;\n      });\n    },\n\n    setDatable() {\n      $(document).ready(() => {\n        $(\"#listproduct\").DataTable({\n          \"scrollY\": '200px',\n          \"scorllX\": false,\n          \"bJQueryUI\": true,\n          \"sPaginationType\": \"full_numbers\",\n          \"bFilter\": true,\n          \"paging\": false,\n          \"scrollCollapse\": true,\n          \"rowReorder\": true,\n          \"destroy\": true,\n          \"bSort\": true,\n          \"language\": {\n            decimal: \"\",\n            emptyTable: \"ไม่พบการข้อมูลจากฐานข้อมูล\",\n            info: \"พบข้อมูล _TOTAL_ เรคอร์ด\",\n            // \"info\": \"แสดง _START_ - _END_ แถว จากทั้งหมด _TOTAL_ แถว\",\n            infoEmpty: \"ไม่พบข้อมูลที่ค้นหา\",\n            infoFiltered: \"(จากข้อมูลทั้งหมด _MAX_ เรคอร์ด)\",\n            infoPostFix: \"\",\n            thousands: \",\",\n            lengthMenu: \"แสดงครั้งละ _MENU_ เรคอร์ด\",\n            loadingRecords: \"Loading...\",\n            processing: \"\",\n            search: \"ค้นหาข้อมูล:\",\n            zeroRecords: \"ไม่พบข้อมูลที่ค้นหา\",\n            paginate: {\n              first: \"หน้าแรก\",\n              last: \"หน้าสุดท้าย\",\n              next: \"ต่อไป\",\n              previous: \"ย้อนกลับ\"\n            },\n            aria: {\n              sortAscending: \": activate to sort column ascending\",\n              sortDescending: \": activate to sort column descending\"\n            }\n          }\n        });\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsHA,OAAOA,CAAP,MAAc,QAAd;AACA,OAAOC,KAAP,MAAkB,SAAlB,C,CACA;;AAEA,eAAe;EACXC,IAAI,GAAG;IACH,OAAO;MACHC,mBAAmB,EAAE,KADlB;MAEHC,mBAAmB,EAAE,EAFlB;MAGHC,YAAY,EAAE,EAHX;MAIHC,aAAa,EAAE,EAJZ;MAKHC,YAAY,EAAE,EALX;MAMHC,SAAS,EAAE,EANR;MAOHC,gBAAgB,EAAE;QACdC,kBAAkB,EAAE,EADN;QAEdC,YAAY,EAAE,EAFA;QAGdC,WAAW,EAAE;MAHC;IAPf,CAAP;EAaH,CAfU;;EAgBX,MAAMC,OAAN,GAAgB;IACZ,MAAM,KAAKC,UAAL,EAAN;IACA,MAAM,KAAKC,UAAL,EAAN;IACA,MAAM,KAAKC,iBAAL,EAAN;IACA,MAAM,KAAKC,cAAL,EAAN;IACA,MAAM,KAAKC,aAAL,EAAN;IACA,MAAM,KAAKC,uBAAL,EAAN;IACA,MAAM,KAAKC,sBAAL,EAAN;EACH,CAxBU;;EAyBXC,OAAO,EAAE;IACLC,UAAU,GAAG;MACT,KAAKnB,mBAAL,GAA2B,CAAC,KAAKA,mBAAjC;IACH,CAHI;;IAILgB,uBAAuB,GAAG;MACtBnB,CAAC,CAACuB,QAAD,CAAD,CAAYC,KAAZ,CAAkB,MAAM;QACpBxB,CAAC,CAAC,eAAD,CAAD,CAAmByB,OAAnB,CAA2B;UACvBC,KAAK,EAAE,aADgB;UAEvBC,aAAa,EAAE,IAFQ;UAGvBC,KAAK,EAAE,MAHgB;UAIvBC,cAAc,EAAE7B,CAAC,CAAC,eAAD,CAAD,CAAmB8B,MAAnB,EAJO;UAKvBC,WAAW,EAAE,qBALU,CAMvB;;QANuB,CAA3B;QAQA/B,CAAC,CAAC,eAAD,CAAD,CAAmBgC,EAAnB,CAAsB,gBAAtB,EAAyCC,CAAD,IAAO;UAC3C,IAAI/B,IAAG,GAAI+B,CAAC,CAACC,MAAF,CAAShC,IAApB;UACA,KAAKO,gBAAL,CAAsBE,YAAtB,GAAqCT,IAAI,CAACiC,EAA1C;QACH,CAHD;MAIH,CAbD;IAcH,CAnBI;;IAoBLf,sBAAsB,GAAG;MACrBpB,CAAC,CAACuB,QAAD,CAAD,CAAYC,KAAZ,CAAkB,MAAM;QACpBxB,CAAC,CAAC,cAAD,CAAD,CAAkByB,OAAlB,CAA0B;UACtBC,KAAK,EAAE,aAD