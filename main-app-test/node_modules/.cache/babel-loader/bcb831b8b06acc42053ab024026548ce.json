{"ast":null,"code":"//import axios from 'axios';\nimport $ from \"jquery\";\nimport axios from \"@/axios\";\nimport { Modal } from \"bootstrap\";\nexport default {\n  data() {\n    return {\n      data_row: [],\n      rubBrand: {\n        rub_brand_id: \"\",\n        rub_brand_shotname: \"\",\n        rub_brand_name: \"\"\n      },\n      rubBrand_edit: {\n        rub_brand_id: \"\",\n        rub_brand_shotname: \"\",\n        rub_brand_name: \"\"\n      }\n    };\n  },\n\n  mounted() {\n    this.getRubberBrandData();\n  },\n\n  methods: {\n    async getRubberBrandData() {\n      await axios.get(`rubberbrand/read`).then(response => {\n        this.data_row = response.data;\n        $(document).ready(function () {\n          $('#showrubberbrand').DataTable({\n            \"language\": {\n              \"decimal\": \"\",\n              \"emptyTable\": \"ไม่พบการข้อมูลจากฐานข้อมูล\",\n              \"info\": \"จำนวน _END_ เรคอร์ด / _TOTAL_ เรคอร์ด\",\n              // \"info\": \"แสดง _START_ - _END_ แถว จากทั้งหมด _TOTAL_ แถว\",\n              \"infoEmpty\": \"Showing 0 to 0 of 0 entries\",\n              \"infoFiltered\": \"(ค้นหาจากข้อมูลทั้งหมด _MAX_ เรคอร์ด)\",\n              \"infoPostFix\": \"\",\n              \"thousands\": \",\",\n              \"lengthMenu\": \"แสดงครั้งละ _MENU_ เรคอร์ด\",\n              \"loadingRecords\": \"Loading...\",\n              \"processing\": \"\",\n              \"search\": \"ค้นหาข้อมูล:\",\n              \"zeroRecords\": \"ไม่พบข้อมูลที่ค้นหา\",\n              \"paginate\": {\n                \"first\": \"First\",\n                \"last\": \"Last\",\n                \"next\": \"ต่อไป\",\n                \"previous\": \"ย้อนกลับ\"\n              },\n              \"aria\": {\n                \"sortAscending\": \": activate to sort column ascending\",\n                \"sortDescending\": \": activate to sort column descending\"\n              }\n            }\n          });\n        });\n      });\n    },\n\n    async newRubBrand() {\n      if (this.rubBrand.rub_brand_shotname == '' || this.rubBrand.rub_brand_name == '') {\n        alert(\"กรุณากรอกช้อมูลให้ครบถ้วนก่อนการบันทึก !!!\");\n      } else {\n        if (confirm(\"ยืนยันการบันทึกข้อมูล ?\") == true) {\n          await axios.post(`rubberbrand/new`, this.rubBrand).then(() => {\n            this.rubBrand = {\n              rub_brand_id: \"\",\n              rub_brand_shotname: \"\",\n              rub_brand_name: \"\"\n            };\n            this.hrub_brand_ideModal(\"newBrand\");\n            $(\"#showrubberbrand\").DataTable().destroy();\n            this.getRubberBrandData();\n            console.log('Process success!');\n          }).catch(error => {\n            console.log(error);\n          });\n        }\n      }\n    },\n\n    async editRubBrand() {\n      if (this.rubBrand_edit.rub_brand_shotname == '' || this.rubBrand_edit.rub_brand_name == '') {\n        alert(\"กรุณากรอกช้อมูลให้ครบถ้วนก่อนการบันทึก !!!\");\n      } else {\n        if (confirm(\"ยืนยันการเปลี่ยนแปลงข้อมูล ?\") == true) {\n          await axios.post(`rubberbrand/update`, this.rubBrand_edit).then(() => {\n            this.rubBrand_edit = {\n              rub_brand_id: \"\",\n              rub_brand_shotname: \"\",\n              rub_brand_name: \"\"\n            };\n            this.hrub_brand_ideModal(\"editBrand\");\n            $(\"#showrubberbrand\").DataTable().destroy(); //--- ทำลาย Datatable\n\n            this.getRubberBrandData();\n          });\n        }\n      }\n    },\n\n    getRubBrandToEdit(val) {\n      this.rubBrand_edit.rub_brand_id = val.rub_brand_id;\n      this.rubBrand_edit.rub_brand_shotname = val.rub_brand_shotname;\n      this.rubBrand_edit.rub_brand_name = val.rub_brand_name;\n      this.showModal(\"editBrand\");\n    },\n\n    async showModal(e) {\n      let myModal = await new Modal(document.getElementByrub_brand_id(e));\n      await myModal.show();\n    },\n\n    async hideModal(e) {\n      await $(`#${e}`).removeClass(\"in\");\n      await $(\".modal-backdrop\").remove();\n      await $(`#${e}`).hrub_brand_ide();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkHA;AACA,OAAOA,CAAP,MAAc,QAAd;AACA,OAAOC,KAAP,MAAkB,SAAlB;AACA,SAASC,KAAT,QAAsB,WAAtB;AAEA,eAAe;EACVC,IAAI,GAAG;IACF,OAAO;MACFC,QAAQ,EAAE,EADR;MAEFC,QAAQ,EAAE;QACLC,YAAY,EAAE,EADT;QAELC,kBAAkB,EAAE,EAFf;QAGLC,cAAc,EAAE;MAHX,CAFR;MAOFC,aAAa,EAAE;QACVH,YAAY,EAAE,EADJ;QAEVC,kBAAkB,EAAE,EAFV;QAGVC,cAAc,EAAE;MAHN;IAPb,CAAP;EAaJ,CAfS;;EAgBVE,OAAO,GAAG;IACL,KAAKC,kBAAL;EACJ,CAlBS;;EAmBVC,OAAO,EAAE;IACJ,MAAMD,kBAAN,GAA2B;MACtB,MAAMV,KAAK,CAACY,GAAN,CAAW,kBAAX,EAA8BC,IAA9B,CAAoCC,QAAD,IAAc;QAClD,KAAKX,QAAL,GAAgBW,QAAQ,CAACZ,IAAzB;QACAH,CAAC,CAACgB,QAAD,CAAD,CAAYC,KAAZ,CAAkB,YAAY;UACzBjB,CAAC,CAAC,kBAAD,CAAD,CAAsBkB,SAAtB,CAAgC;YAC3B,YAAY;cACP,WAAW,EADJ;cAEP,cAAc,4BAFP;cAGP,QAAQ,uCAHD;cAIP;cACA,aAAa,6BALN;cAMP,gBAAgB,uCANT;cAOP,eAAe,EAPR;cAQP,aAAa,GARN;cASP,cAAc,4BATP;cAUP,kBAAkB,YAVX;cAWP,cAAc,EAXP;cAYP,UAAU,cAZH;cAaP,eAAe,qBAbR;cAcP,YAAY;gBACP,SAAS,OADF;gBAEP,QAAQ,MAFD;gBAGP,QAAQ,OAHD;gBAIP,YAAY;cAJL,CAdL;cAoBP,QAAQ;gBACH,iBAAiB,qCADd;gBAEH,kBAAkB;cAFf;YApBD;UADe,CAAhC;QA2BJ,CA5BD;MA6BJ,CA/BK,CAAN;IAgCJ,CAlCG;;IAmCJ,MAAMC,WAAN,GAAoB;MACf,IAAI,KAAKd,QAAL,CAAcE,kBAAd,IAAoC,EAApC,IAA0C,KAAKF,QAAL,CAAcG,cAAd,IAAgC,EAA9E,EAAkF;QAC7EY,KAAK,CAAC,4CAAD,CAAL;MACL,CAFA,MAEO;QACF,IAAIC,OAAO,CAAC,yBAAD,CAAP,IAAsC,IAA1C,EAAgD;UAC3C,MAAMpB,KAAK,CAACqB,IAAN,CAAY,iBAAZ,EAA8B,KAAKjB,QAAnC,EAA6CS,IAA7C,CAAkD,MAAM;YACzD,KAAKT,QAAL,GAAgB;cACXC,YAAY,EAAE,EADH;cAEXC,kBAAkB,EAAE,EAFT;cAGXC,cAAc,EAAE;YAHL,CAAhB;YAKA,KAAKe,mBAAL,CAAyB,UAAzB;YACAvB,CAAC,CAAC,kBAAD,CAAD,CAAsBkB,SAAtB,GAAkCM,OAAlC;YACA,KAAKb,kBAAL;YACAc,OAAO,CAACC,GAAR,CAAY,kBAAZ;UACJ,CAVK,EAUHC,KAVG,CAUGC,KAAI,IAAK;YACbH,OAAO,CAACC,GAAR,CAAYE,KAAZ;UACJ,CAZK,CAAN;QAaL;MACL;IACJ,CAvDG;;IAwDJ,MAAMC,YAAN,GAAqB;MAChB,IAAI,KAAKpB,aAAL,CAAmBF,kBAAnB,IAAyC,EAAzC,IAA+C,KAAKE,aAAL,CAAmBD,cAAnB,IAAqC,EAAxF,EAA4F;QACvFY,KAAK,CAAC,4CAAD,CAAL;MACL,CAFA,MAEO;QACF,IAAIC,OAAO,CAAC,8BAAD,CAAP,IAA2C,IAA/C,